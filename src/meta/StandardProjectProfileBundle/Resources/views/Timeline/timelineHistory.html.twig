<form action="" method="GET">
  <select>
    <option>Updates</option>
    <option>Comments</option>
    <option>Users</option>
  </select>
</form>
<ul class="timeline">
{% for step in timeframe %}
  {% if step.data|length > 0 %}
  <li class="dateWrapper"><div class="date-wrapper"><div class="date">{{ step.name}}</div></div></li>

  {% for item in step.data %}
    <li class="logWrapper"><div class="log">{{ item|raw }}</div></li>
  {% endfor %}

  {% else %}
  <li class="dateWrapper pass"></li>
  {% endif %}
{% endfor %}
</ul>