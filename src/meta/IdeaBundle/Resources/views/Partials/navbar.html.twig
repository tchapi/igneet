{% spaceless %}
  <ul>
    {% for key, menuItem in menu %}
      {% if not menuItem.private or canEdit %}
      <li{% if activeMenu == key %} class="active"{% endif %}><a href="{{ path(menuItem.route, {'uid': uid }) }}">
        <i class="{{ menuItem.icon }}"></i> {{ ("idea." ~ key ~ ".title")|trans }}</a></li>
      {% endif %}
    {% endfor %}
  </ul>
{% endspaceless %}
